    <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table{
            border: 1px solid #000;

        }
        th,td{
            border: 1px solid #000;
        }
    </style>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>الاسم</th>
                <th>الملاحظات</th>
                <th>المقترحات</th>
            </tr>
        </thead>

        <tbody>
            <% data.forEach(x=>{%>
            <tr>
                <td><%= x.name%></td>
                <td><%= x?.rate?.notes || "لا يوجد"%> </td>
                <td><%=x?.rate?.suggestions|| "لا يوجد" %> </td>
            </tr>
            <% })%>
            
        </tbody>

    </table>
    
</body>
</html>