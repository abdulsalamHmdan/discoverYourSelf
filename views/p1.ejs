        function loadProgress(userName) {
            const savedData = localStorage.getItem(`riasec_progress_${userName}`);
            if (savedData) {
                const progressData = JSON.parse(savedData);
                // ุงูุชุญูู ูู ุฃู ุงูุจูุงูุงุช ููุณุช ูุฏููุฉ ุฌุฏุงู (ุฃูุซุฑ ูู 7 ุฃูุงู)
                const daysSinceLastSave = (Date.now() - progressData.lastSaved) / (1000 * 60 * 60 * 24);
                if (daysSinceLastSave <= 7) {
                    return progressData;
                }
            }
            return null;
        }

